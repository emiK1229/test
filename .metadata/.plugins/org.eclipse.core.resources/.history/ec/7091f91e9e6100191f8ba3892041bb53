package person;

public class Test {

//演習①
	public static void main(String[]args){
Person taro = new Person();
taro.name="山田太郎";
taro.age=20;

System.out.println(taro.name);
System.out.println(taro.age);
	}

//演習②
taro.name2="木村次郎";
taro.age2=16;

System.out.println(taro2.name);
System.out.println(taro2.age);

}
